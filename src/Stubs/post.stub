function({ data = {}, headers = {}, params = {} }) {
        return fetch(route('{{routeName}}', params), {
            method: '{{method}}',
            headers: {
                'X-CSRF-TOKEN':  document.querySelector('meta[name="csrf"]').content,
                'Content-Type': 'application/json',
                ...headers
            },
            body: JSON.stringify(data),
        })
    },
