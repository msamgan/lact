function({ data = {}, headers = {}, param = {} }) {
        return fetch(route('{{routeName}}', param), {
            method: 'PATCH',
            headers: {
                'X-CSRF-TOKEN':  document.querySelector('meta[name="csrf"]').content,
                'Content-Type': 'application/json',
                ...headers
            },
            body: JSON.stringify(data),
        }).then(response => response)
    },
